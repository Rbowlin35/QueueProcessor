<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler,log4net" />
  </configSections>

  <appSettings>
    <add key="ConnectionString" value="data source=.\SQLEXPRESS;Initial Catalog=SandBox;Integrated Security=True;MultipleActiveResultSets=True" />
